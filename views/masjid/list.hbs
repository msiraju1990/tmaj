<h3><a class="btn btn-secondary" href="/masjid"><i class="fa fa-plus"></i> Create New</a> Masjid List</h3>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Full Name</th>
            <th>Address</th>
            <th>City</th>
            <th>Pin Code</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.fullName}}</td>
            <td>{{this.address}}</td>
            <td>{{this.city}}</td>
            <td>{{this.pincode}}</td>
            <td>
                <a href="/masjid/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/masjid/delete/{{this._id}}" onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>